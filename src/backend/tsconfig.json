{
    "compilerOptions": {
        "outDir": "../../dist/backend/",
        "sourceMap": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "strict": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
				"noUnusedLocals": true,
				"noUnusedParameters": true,
				"lib": [
					"es2017",
					"dom",
          "esnext.asynciterable"
				]
     },
    "include": [
        "./**/*"
    ],
		"exclude": [
			"node_modules",
			"./**/*.test.*"
		]
}
